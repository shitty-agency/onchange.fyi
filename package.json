{
  "name": "onchange.fyi",
  "version": "0.0.0",
  "description": "frontend",
  "main": "index.html",
  "scripts": {
    "precommit": "yarn build && lint-staged",
    "start": "parcel index.html",
    "clean": "rm -rf docs/*.js docs/*.html docs/*.map dist coverage .cache .nyc_output",
    "prebuild": "yarn clean",
    "build": "parcel build index.html",
    "prod": "(cd docs && python -m SimpleHTTPServer 8000)",
    "postbuild": "mkdir docs || true && cp dist/*.js docs/ && cp dist/*.html docs/",
    "test": "ava",
    "lint": "eslint . --fix",
    "coverage": "nyc ava",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/one19/onchange.fyi.git"
  },
  "keywords": [
    "frontend",
    "react"
  ],
  "author": "Drew Showalter <maelstroswim@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/one19/onchange.fyi/issues"
  },
  "homepage": "https://github.com/one19/onchange.fyi#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@brightsole/eslint-config-lint-front": "^0.0.5",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-info": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "a-plus-forms": "^0.7.17",
    "a-plus-forms-json-validator": "^1.0.6",
    "ava": "^1.0.0-beta.8",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "chroma-js": "^1.3.5",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "husky": "^1.1.2",
    "jsdom": "^12.0.0",
    "lint-staged": "^7.2.2",
    "localforage": "^1.7.2",
    "nanoid": "^1.3.1",
    "nock": "^10.0.1",
    "nyc": "^13.0.1",
    "parcel-bundler": "^1.10.3",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "styled-components": "^4.0.3"
  },
  "dependencies": {}
}
